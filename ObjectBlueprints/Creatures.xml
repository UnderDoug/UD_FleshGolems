    <intproperty Name="CyberneticRejectionSyndromeModifier" Value="100" />
    <inventoryobject Blueprint="CyberneticsCreditWedge3" Number="1-2" IntProperties="AlwaysEquipAsWeapon:1,WontSell:1" />
