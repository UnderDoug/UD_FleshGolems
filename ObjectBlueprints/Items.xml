<?xml version="1.0" encoding="utf-8" ?>
<objects>
  <object Name="Corpse" Load="Merge">
    <part Name="UD_FleshGolems_CorpseReanimationHelper" />
    <tag Name="Animatable" />
    <tag Name="Tier" />
    <builder Name="Animated" ChanceOneIn="3000" />
    <tag Name="HeroNameTitleDescriptor" Value="Suffering,Waiting,Silent" />
    <tag Name="HeroNameTitleAdmirers" Value="Surgeons,Healers,Scientists,Scientists of the Mad Variety" />
    <tag Name="DynamicObjectsTable:AnimatableFurniture" />
    <tag Name="BodyType" Value="Humanoid" />
    <xtagTextFragments
      Skin="leather"
      PoeticFeatures="viscera,muck,entrails,sinew,bone"
      PoeticVerbs="squirming,wriggling,squelching"
      PoeticAdjectives="wet,moist,meaty"
      PoeticnNoises="gurgles,slurps,squelches" />
    <tag Name="AnimatedSkills" Value="CookingAndGathering,CookingAndGathering_Butchery,Persuasion_Intimidate,Multiweapon_Fighting,ShortBlades,Axes,Cudgels,LongBlades" />
    <tag Name="UD_FleshGolems LimbType" Value="*"/>
    <tag Name="UD_FleshGolems CyberneticsSlot" Value="*"/>
  </object>
  
  <object Name="Robot Corpse" Load="Merge">
    <tag Name="HeroNameTitleDescriptor" Value="Scuffed,Broken-down,Corrupted" />
    <tag Name="HeroNameTitleAdmirers" Value="Tinkers,Craftspeople,Robots" />
    <tag Name="BodyType" Value="HumanoidRobot" />
    <xtagTextFragments
      Skin="shorn metal"
      PoeticFeatures="oil,gel,bent chrome,jagged edges"
      PoeticVerbs="jittering,stuttering,whining"
      PoeticAdjectives="slick,erratic,crunchy"
      PoeticnNoises="sizzles,whirs,sputters" />
  </object>
  
  <object Name="UD_FleshGolems PlantCorpseHeroics">
    <tag Name="HeroNameTitleDescriptor" Value="Wilted,Composted,Blighted" />
    <tag Name="HeroNameTitleAdmirers" Value="Gardeners,Arborists" />
    <tag Name="BaseObject" Value="*noinherit" />
    <tag Name="ExcludeFromDynamicEncounters" Value="*noinherit" />
  </object>
  <object Name="UD_FleshGolems Plant Corpse" Inherits="Corpse">
    <part Name="Render" DisplayName="Plant Corpse" Tile="items/sw_splat1.bmp"
          TileColor="&amp;g" DetailColor="w" RenderString="%"
          ColorString="&amp;g" RenderLayer="5" />
    <part Name="Description" Short="A mound of dank foliage and compost puts some elevation to the ground." />
    <part Name="Physics" Weight="10" />
    <mixin Name="UD_FleshGolems PlantCorpseHeroics" />
    <tag Name="Plant" />
    <removetag Name="Meat" />
    <part Name="Food" Message="The fibers are tough." />
    <xtagTextFragments
      Skin="fibres"
      PoeticFeatures="sap,fibre,roots"
      PoeticVerbs="tangling,stuttering,wriggling"
      PoeticAdjectives="damp,moist,mulchy"
      PoeticnNoises="gurgles,slurps,squelches" />
    <tag Name="ExcludeFromDynamicEncounters" Value="*noinherit" />
  </object>
